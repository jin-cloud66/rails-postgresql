<h1>Listing Users</h1>

<%= link_to 'New user', new_user_path %>
 
<table>
  <tr>
    <th>User Name</th>
    <th>User Email</th>
    <th></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>
